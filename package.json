{
  "name": "devops-practice",
  "devDependencies": {
    "semantic-release-preconfigured-conventional-commits": "1.1.15"
  },
  "engines": {
    "node": "18.12"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}